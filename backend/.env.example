# Environment config
ENVIRONMENT=development

# LLM config
SHIPMENT_INTELLIGENCE_LLM_PROVIDER=openai
SHIPMENT_INTELLIGENCE_LLM_MODEL=gpt-4o-mini
SHIPMENT_INTELLIGENCE_LLM_API_KEY=

# Embedding config
SHIPMENT_INTELLIGENCE_EMBEDDING_PROVIDER=google_genai
SHIPMENT_INTELLIGENCE_EMBEDDING_MODEL=models/gemini-embedding-001
SHIPMENT_INTELLIGENCE_EMBEDDING_API_KEY=

# Sparse embedding config
SHIPMENT_INTELLIGENCE_SPARSE_EMBEDDING_PROVIDER=fastembed
SHIPMENT_INTELLIGENCE_SPARSE_EMBEDDING_MODEL=prithivida/Splade_PP_en_v1

# Qdrant config
SHIPMENT_INTELLIGENCE_QDRANT_MODE=remote
# SHIPMENT_INTELLIGENCE_QDRANT_PATH=./qdrant_data
SHIPMENT_INTELLIGENCE_QDRANT_URL=http://qdrant:6333
SHIPMENT_INTELLIGENCE_QDRANT_API_KEY=mysecretapikey
SHIPMENT_INTELLIGENCE_QDRANT_COLLECTION=shipment_events