# Environment config
ENVIRONMENT=development

# LLM config
SHIPMENT_INTELLIGENCE_LLM_PROVIDER=ollama
SHIPMENT_INTELLIGENCE_LLM_MODEL=llama3.1:8b
SHIPMENT_INTELLIGENCE_LLM_API_KEY=

# Embedding config
SHIPMENT_INTELLIGENCE_EMBEDDING_PROVIDER=google_genai
SHIPMENT_INTELLIGENCE_EMBEDDING_MODEL=models/gemini-embedding-001
SHIPMENT_INTELLIGENCE_EMBEDDING_API_KEY=

# Qdrant config
SHIPMENT_INTELLIGENCE_QDRANT_MODE=local
SHIPMENT_INTELLIGENCE_QDRANT_PATH=./qdrant_data
# SHIPMENT_INTELLIGENCE_QDRANT_URL=http://localhost:6333
# SHIPMENT_INTELLIGENCE_QDRANT_API_KEY=your_api_key_here
SHIPMENT_INTELLIGENCE_QDRANT_COLLECTION=shipment_events